{
    "version": 2,
    "builds": [
      {
        "src": "api/send-location.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/send-location",
        "dest": "/api/send-location.py"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  